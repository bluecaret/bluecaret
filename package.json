  "license": "unlicensed",
  "homepage": "https://bluecaret.github.io/bluecaret",
  "dependencies": {
    "normalize-scss": "^7.0.1"
  },
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5"
  }
}
